  "scripts": {
    "dev": "next dev -p 8080 --experimental-https",
    "prebuild": "git submodule update --remote && npm run build:article && npm run build:similarity && npm run build:popular",
    "build": "TZ='Asia/Tokyo' next build",
    "postbuild": "npm run build:feed && npm run build:sitemap && npm run build:ogp",
