{
  "excludeFiles": [
    "./src/**/*.ts",
    "./src/_app.tsx",
    "./src/_document.tsx"
  ],
  "parser": {
    "\\.[jt]sx?$": "@markuplint/jsx-parser"
  },
  "specs": {
    "\\.[jt]sx?$": "@markuplint/react-spec"
  },
  "extends": [
    "markuplint:recommended-react"
  ],
  "nodeRules": [
    {
      "selector": "meta[property]",
      "rules": {
        "invalid-attr": {
          "options": {
            "attrs": {
              "property": {
                "type": "Any"
              },
              "content": {
                "type": "Any"
              }
            }
          }
        }
      }
    }
  ]
}
